@Html.Partial("_QuestionModal")
@section Scripts {
    <script>
        $(function () {
            if (detectMobileDevice() == 1) {
                //$("#includedContentMenuMobile").load("/arena20/includes/menu-logged-mobile.html");
                //$("#includedContentMenuRankingMobile").load("menu-my-matches-mobile.html");
                hidden_show_ObjectMobile("footerMobile", "block");
            }
        });

        var msgStandardWaiting = "Wait, process in progress...";

        function includeFileScreenModalToPrepareSubmit() {
            $("#modalScreeenDeleteTitle").html("PREPARE STAGING/BKP DATABASES");
            $("#modalScreeenDeleteSubTitle").html("");
            $("#modalScreeenDeleteQuestion").html("Would you like to prepare the staging & bkp databases before to generate new season?");
            $("#modalScreeenDeleteButton").on("click", function () { submitAction('prepare-databases-before', 'Preparing Database BKP in progress...'); });
        }

        function includeFileScreenModalToGenerateSubmit() {
            $("#modalScreeenDeleteTitle").html("GENERATE NEW SEASON");
            $("#modalScreeenDeleteSubTitle").html("");
            $("#modalScreeenDeleteQuestion").html("Would you like to generate new season?");
            $("#modalScreeenDeleteButton").on("click", function () { submitAction('generate-new-season', 'Generating New Season - Prepare Database...'); });
        }

        function includeFileScreenModalToCreateSpoolerSubmit() {
            $("#modalScreeenDeleteTitle").html("NEW SEASON GENERATED - SEND EMAILS");
            $("#modalScreeenDeleteSubTitle").html("");
            $("#modalScreeenDeleteQuestion").html("Would you like to create spooler of emails to the new season generated?");
            $("#modalScreeenDeleteButton").on("click", function () { submitAction('create-spooler', ''); });
        }

        function includeFileScreenModalToCancelSpoolerSubmit() {
            $("#modalScreeenDeleteTitle").html("SPOOLER - CANCEL");
            $("#modalScreeenDeleteSubTitle").html("");
            $("#modalScreeenDeleteQuestion").html("Would you like to cancel spooler of emails to the new season generated?");
            $("#modalScreeenDeleteButton").on("click", function () { submitAction('cancel-spooler', ''); });
        }

        function includeFileScreenModalToCancelRenewalSubmit() {
            $("#modalScreeenDeleteTitle").html("NEW SEASON - CANCEL");
            $("#modalScreeenDeleteSubTitle").html("");
            $("#modalScreeenDeleteQuestion").html("Would you like to cancel the processes of new season generated?");
            $("#modalScreeenDeleteButton").on("click", function () { submitAction('cancel-new-season', ''); });
        }

        function submitAction(actionForm, msgWaiting) {
            if (msgWaiting == '') { msgWaiting = msgStandardWaiting; }
            $("#actionForm").val(actionForm);
            $("#spWaiting").html(msgWaiting);
            $("#waiting").show();
            $("#registration-form-new-season").submit();
        }


        $(document).ready(function () {
            if ('@TempData["returnMessage"]' != '')
                toastr.error('Aviso: @TempData["returnMessage"].');

            if ('@TempData["actionSuccessfully"]' != '')
                toastr.success('@TempData["actionSuccessfully"].');

            if (detectMobileDevice() == 0)
                activeSidebarMenu("/Arena20/BatchServices/GenerateNewSeasonShowView");

            $("div.gentelella-right_col").find("input[type=textAmount]").attr("style", "width:50px;text-align:center");
            $("code").attr("style", "height:0px;background-color:#FFF")
            $("#waiting").hide();

            if ('@Model.DatabasesLookTheSame'== 'True') {
                $("button.gentelella-btn-warning").attr('disabled', 'true');
            }
            else {
                $("button.gentelella-btn-warning").on("click", function () {
                    includeFileScreenModalToPrepareSubmit();
                    $(this).attr("data-toggle", "modal");
                    $(this).attr("data-target", "#bs-delete-modal-sm");
                    $('#bs-delete-modal-sm').modal('show');
                    $(this).attr("data-toggle", "");
                    $(this).attr("data-target", "");
                });
            }

            if ('@Model.NewSeasonIsGenerated'== 'True') {
                $("button.gentelella-btn-success").attr('disabled', 'true');
                $("button.gentelella-btn-danger[target='newSeason']").on("click", function () {
                    includeFileScreenModalToCancelRenewalSubmit();
                    $(this).attr("data-toggle", "modal");
                    $(this).attr("data-target", "#bs-delete-modal-sm");
                    $('#bs-delete-modal-sm').modal('show');
                    $(this).attr("data-toggle", "");
                    $(this).attr("data-target", "");
                });
            }
            else {
                $("button.gentelella-btn-danger[target='newSeason']").attr('disabled', 'true');
                if ('@Model.DatabasesLookTheSame' == 'True') {
                    $("button.gentelella-btn-success").on("click", function () {
                        includeFileScreenModalToGenerateSubmit();
                        $(this).attr("data-toggle", "modal");
                        $(this).attr("data-target", "#bs-delete-modal-sm");
                        $('#bs-delete-modal-sm').modal('show');
                        $(this).attr("data-toggle", "");
                        $(this).attr("data-target", "");
                    });
                }
                else
                    $("button.gentelella-btn-success").attr('disabled', 'true');
            }

            if ('@Model.emailsSent'== 'True') {
                $("button.gentelella-btn-info").attr('disabled', 'true');
                $("button.gentelella-btn-danger[target='spooler']").on("click", function () {
                    includeFileScreenModalToCancelSpoolerSubmit();
                    $(this).attr("data-toggle", "modal");
                    $(this).attr("data-target", "#bs-delete-modal-sm");
                    $('#bs-delete-modal-sm').modal('show');
                    $(this).attr("data-toggle", "");
                    $(this).attr("data-target", "");
                });
            }
            else {
                $("button.gentelella-btn-danger[target='spooler']").attr('disabled', 'true');
                if ('@Model.NewSeasonIsGenerated' == 'True') {
                    $("button.gentelella-btn-info").on("click", function () {
                        includeFileScreenModalToCreateSpoolerSubmit();
                        $(this).attr("data-toggle", "modal");
                        $(this).attr("data-target", "#bs-delete-modal-sm");
                        $('#bs-delete-modal-sm').modal('show');
                        $(this).attr("data-toggle", "");
                        $(this).attr("data-target", "");
                    });
                }
                else
                    $("button.gentelella-btn-info").attr('disabled', 'true');
            }

            if ('@Model.actionUser'.indexOf("generate-new-season") > -1) {
                var strMsgWaiting = '';

                if ('@Model.actionUser' == 'generate-new-season-caulculate-h2h')
                    strMsgWaiting = "Wait, generating new season - calculate H2H season in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-caulculate-fut')
                    strMsgWaiting = "Wait, generating new season - calculate FUT season in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-caulculate-pro')
                    strMsgWaiting = "Wait, generating new season - calculate PRO CLUB season in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-leagues-h2h')
                    strMsgWaiting = "Wait, generating new season - generate H2H league championships in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-cups-h2h')
                    strMsgWaiting = "Wait, generating new season - generate H2H cup championships in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-fut')
                    strMsgWaiting = "Wait, generating new season - generate FUT championships in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-pro')
                    strMsgWaiting = "Wait, generating new season - generate PRO CLUB championships in progress...";
                else if ('@Model.actionUser' == 'generate-new-season-maintenance')
                    strMsgWaiting = "Wait, generating new season - maintenance and preparation of the database in progress...";

                if (strMsgWaiting != '') {
                    $("button.gentelella-btn-success").attr('disabled', 'true');
                    submitAction('@Model.actionUser', strMsgWaiting);
                }

                $("html, body").animate({ scrollTop: $('#animate-waiting-process').offset().top }, "slow");
            }
            else if ('@Model.actionUser' == 'ERROR') {
                $('.bs-callout-warning').toggleClass('gentelella-hidden', false);
                $("div.bs-callout-warning").html('<b>ERROR OCURRED IN THE PROCESS: </b>' + '<br /><br />' + '@TempData["returnMessage"]');
                $("html, body").animate({ scrollTop: $('div.bs-callout-warning').offset().top }, "slow");
            }
        });

    </script>
}

