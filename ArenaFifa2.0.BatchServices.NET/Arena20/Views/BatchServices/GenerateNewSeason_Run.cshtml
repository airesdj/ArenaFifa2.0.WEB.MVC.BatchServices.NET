@using ArenaFifa20.BatchServices.NET.Models
@model GenerateNewSeasonGenerateModel
@{
    ViewBag.Title = "Batch Services - Generate New Season";
}

<!-- page content -->
<div class="gentelella-right_col" role="main" style="padding-left:20px">
    <div class="gentelella-page-title">
        <div class="gentelella-col-md-9 gentelella-col-sm-9 gentelella-col-xs-12">
            <h3>BATCH SERVICES - GENERATE NEW SEASON - RUN</h3>
        </div>
    </div>
    <div class="clearfix">&nbsp;</div>

    @{ TempData["FullModel"] = Model; }
    @Html.Partial("_MessageValidate")

    <form action="/Arena20/BatchServices/GenerateNewSeason_Run" class="form-horizontal" id="registration-form-new-season" data-parsley-validate method="post" role="form">

        <input type="hidden" id="actionForm" name="actionForm" value="SAVE-DETAILS-CHAMPIONSHIP-LEAGUE" />
        @Html.AntiForgeryToken()

        <div class="gentelella-x_panel">
            <div class="gentelella-x_content">

                <div class="gentelella-row">
                    <div class="gentelella-col-md-12 gentelella-col-sm-12 gentelella-col-xs-12 gentelella-a-center">
                        <h3><code>NEW SEASON GENERATED - DETAILS:</code></h3>
                    </div>
                </div>
                <br />
                <div class="gentelella-row">
                    <div class="gentelella-col-md-4 gentelella-col-sm-4 gentelella-col-xs-12">
                        <label for="deacricao">Name :</label>
                        &nbsp; @Model.seasonName
                    </div>
                    <div class="gentelella-col-md-5 gentelella-col-sm-5 gentelella-col-xs-12">
                        <label class="gentelella-control-label" for="dtInicio">Process Date:</label>
                        &nbsp;
                        @if (Model.seasonID > 0)
                        {
                            if (Model.NewSeasonIsGenerated)
                            {
                                <span>
                                    @(Model.startDate.ToString("dd/MM/yyy hh:mm:ss")) &nbsp; &nbsp;<b>TO</b>&nbsp; &nbsp; @(Model.endDate.ToString("dd/MM/yyy hh:mm:ss"))
                                </span>
                            }
                            else
                            {
                                <span>
                                    @(Model.startDate.ToString("dd/MM/yyy hh:mm:ss")) &nbsp; &nbsp;<b>TO</b>&nbsp; &nbsp; (processing...)
                                </span>
                            }
                        }
                    </div>
                </div>
                <div class="gentelella-row">
                    <div class="gentelella-col-md-4 gentelella-col-sm-4 gentelella-col-xs-12">
                        <label for="deacricao">User Name :</label>
                        &nbsp; @Model.userName
                    </div>
                    <div class="gentelella-col-md-2 gentelella-col-sm-2 gentelella-col-xs-12">
                        <label for="deacricao">PSN ID :</label>
                        &nbsp; @Model.psnID
                    </div>
                </div>
                <br />
            </div>
        </div>
